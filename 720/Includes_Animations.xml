<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<!-- Window Animation -->
	<include name="Animation_Window_Dialog">
		<include condition="Skin.hasSetting(Global_Hide_Animation)">Animation_Window_Fade</include>
		<include condition="!Skin.hasSetting(Global_Hide_Animation)">Animation_Window_Zoom</include>
	</include>

	<include name="Animation_Window_Fade">
		<animation effect="fade" start="0" end="100" time="300">WindowOpen</animation>	
		<animation effect="fade" start="100" end="0" time="300">WindowClose</animation>
	</include>

	<include name="Animation_Window_Fade_Delay">
		<animation effect="fade" start="0" end="100" delay="1000" time="300">WindowOpen</animation>	
		<animation effect="fade" start="100" end="0" time="300">WindowClose</animation>
	</include>

	<include name="Animation_Window_Zoom">
		<include>AspectRatio_Window_Zoom</include>
	</include>

	<include name="Animation_Viewmode_Zoom">
		<animation type="WindowOpen">
			<effect type="fade" time="300" start="0" end="100" />
			<effect type="zoom" time="400" center="auto" start="200" end="100" tween="cubic" easing="out" />
		</animation>
		<animation type="WindowClose">
			<effect type="fade" time="300" start="100" end="0" />
			<effect type="zoom" time="400" center="auto" start="100" end="200" tween="cubic" easing="in" />
		</animation>
	</include>

	<include name="Animation_Window_Startup">
		<animation effect="fade" start="0" end="100" time="2000">WindowOpen</animation>	
		<animation effect="fade" start="100" end="0" time="1000">WindowClose</animation>
	</include>

