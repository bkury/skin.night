			<!-- Keyboard Position -->
			<animation effect="slide" end="0,180" condition="Skin.HasSetting(Keyboard_Pos_1) + ![ Skin.HasSetting(Keyboard_Size_1) + Skin.HasSetting(Keyboard_Size_2) + Skin.HasSetting(Keyboard_Size_3) ]">conditional</animation>
			<animation effect="slide" end="0,-35" condition="Skin.HasSetting(Keyboard_Pos_1) + Skin.HasSetting(Keyboard_Size_1)">conditional</animation>
			<animation effect="slide" end="0,-70" condition="Skin.HasSetting(Keyboard_Pos_1) + Skin.HasSetting(Keyboard_Size_2)">conditional</animation>
			<animation effect="slide" end="0,-100" condition="Skin.HasSetting(Keyboard_Pos_1) + Skin.HasSetting(Keyboard_Size_3)">conditional</animation>

			<animation effect="slide" end="0,-180" condition="Skin.HasSetting(Keyboard_Pos_2) + ![ Skin.HasSetting(Keyboard_Size_1) + Skin.HasSetting(Keyboard_Size_2) + Skin.HasSetting(Keyboard_Size_3) ]">conditional</animation>
			<animation effect="slide" end="0,35" condition="Skin.HasSetting(Keyboard_Pos_2) + Skin.HasSetting(Keyboard_Size_1)">conditional</animation>
			<animation effect="slide" end="0,70" condition="Skin.HasSetting(Keyboard_Pos_2) + Skin.HasSetting(Keyboard_Size_2)">conditional</animation>
			<animation effect="slide" end="0,100" condition="Skin.HasSetting(Keyboard_Pos_2) + Skin.HasSetting(Keyboard_Size_3)">conditional</animation>

			<!-- Keyboard Size -->
			<animation effect="zoom" end="120" center="auto" condition="Skin.HasSetting(Keyboard_Size_1)">conditional</animation>
			<animation effect="zoom" end="140" center="auto" condition="Skin.HasSetting(Keyboard_Size_2)">conditional</animation>
			<animation effect="zoom" end="160" center="auto" condition="Skin.HasSetting(Keyboard_Size_3)">conditional</animation>

