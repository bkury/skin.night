<?xml version="1.0" encoding="UTF-8"?>
<includes>

<!-- OSD BOTTOM -->
<include name="OSD_Bottom">

	<control type="group">

		<!-- Plot -->
		<control type="group">
			<include>Animation_CommonFade_600</include>
			<visible>!Skin.HasSetting(OSD_Plot) + ![ isEmpty(VideoPlayer.Plot) | Skin.String(osd_mode,Off) | Skin.String(osd_mode,Minimal) | Window.isVisible(125) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231]) ]</visible>

                        <animation effect="slide" start="0,0" end="0,160" time="800" tween="back" easing="inout" condition="Skin.String(osd_mode,Top)">Conditional</animation>
                        <animation effect="slide" start="0,0" end="0,-482" time="800" tween="back" easing="inout" condition="Skin.String(osd_mode,Bottom)">Conditional</animation>

			<posy>480</posy>       
			<control type="image">
				<width>1280</width>
				<height>80</height>
				<texture border="10" flipy="true">base-rounded.png</texture>
				<colordiffuse>FF000000</colordiffuse>
				<!-- Opacity Option -->
				<include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
				<include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
			</control>
			<control type="image">
                                <posy>0</posy>
                                <width>1280</width>
                                <height>0.5</height>
                                <texture>base-rounded.png</texture>
                                <colordiffuse>mainblue</colordiffuse>
                                <!-- Opacity Option -->
                                <include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
                                <include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
                        </control>
			<control type="textbox">
				<posy>10</posy>
                                <posx>25</posx>
                                <width>900</width>
                                <height>65</height>
                                <align>justify</align>
                                <info>VideoPlayer.Plot</info>
                                <font>Font_Bartowski_Movies_Plot</font>
                                <textcolor>FFCCCCCC</textcolor>
                                <autoscroll time="2500" delay="5000" repeat="5000">Skin.HasSetting(autoscroll)</autoscroll>
                        </control>
		</control>

		<!-- Seekbar Background -->
		<control type="group">
			<include>Animation_CommonFade_600</include>
			<!-- Mode Default -->
			<control type="image">
				<posy>560</posy>
				<width>1280</width>
				<height>180</height>
				<texture border="10" flipy="true">base-rounded.png</texture>
				<colordiffuse>FF000000</colordiffuse>
				<!-- Opacity Option -->
				<include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
				<include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
				<!--<visible>![ Skin.String(osd_mode,Off) | Skin.String(osd_mode,Top) | Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Bottom) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,None)]</visible>-->
				<visible>![ Skin.String(osd_mode,Off) | Skin.String(osd_mode,Top) | Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Bottom) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231])]</visible>
			</control>
			<control type="image">
                                <posy>560</posy>
                                <width>1280</width>
                                <height>0.5</height>
                                <texture>base-rounded.png</texture>
                                <colordiffuse>mainblue</colordiffuse>
                                <!-- Opacity Option -->
                                <include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
                                <include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
				<visible>!Skin.HasSetting(OSD_Clearart) + ![ Skin.String(osd_mode,Off) | Skin.String(osd_mode,Top) | Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Bottom) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231])]</visible>
                        </control>
			<control type="image">
                                <posy>560</posy>
                                <width>930</width>
                                <height>0.5</height>
                                <texture>base-rounded.png</texture>
                                <colordiffuse>mainblue</colordiffuse>
                                <!-- Opacity Option -->
                                <include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
                                <include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
				<visible>Skin.HasSetting(OSD_Clearart) + ![ Skin.String(osd_mode,Off) | Skin.String(osd_mode,Top) | Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Bottom) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231])]</visible>
                        </control>

			<!-- Mode Top -->
			<control type="image">
				<posy>70</posy>
				<width>1280</width>
				<height>35</height>
				<texture border="10" flipy="true">base-rounded.png</texture>
				<colordiffuse>FF000000</colordiffuse>
				<!-- Opacity Option -->
				<include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
				<include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
				<visible>Skin.String(osd_mode,Top)</visible>
			</control>

			<!-- Mode Minimal / Bottom-->
			<control type="image">
				<posy>680</posy>
				<width>1280</width>
				<height>40</height>
				<texture border="10" flipy="true">base-rounded.png</texture>
				<colordiffuse>FF000000</colordiffuse>
				<!-- Opacity Option -->
				<include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
				<include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
				<visible>Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Bottom) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231])</visible>
			</control>
			<control type="image">
                                <posy>78</posy>
                                <width>1280</width>
                                <height>0.5</height>
                                <texture>base-rounded.png</texture>
                                <colordiffuse>mainblue</colordiffuse>
                                <!-- Opacity Option -->
                                <include condition="isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity_Default</include>
                                <include condition="!isEmpty(Skin.String(OSD_Opacity))">OSD_Opacity</include>
				<visible>!Skin.HasSetting(OSD_Plot) + Skin.String(osd_mode,Bottom)</visible>
                        </control>

		</control>

		<!-- Seekbar -->
		<control type="group">
			<posy>560</posy>
			<animation effect="slide" start="0,0" end="0,-610" time="800" tween="back" easing="inout" condition="Skin.String(osd_mode,Top)">Conditional</animation>
			<include>Animation_CommonFade_600</include>
			<visible>!Skin.String(osd_mode,Off)</visible>

			<!-- Progress Background -->
			<control type="image">
				<posx>100</posx>
				<posy>130</posy>
				<width>1080</width>
				<height>20</height>
				<texture>osd_progress_back_normal.png</texture>
			</control>

			<!-- Progress Bar -->
			<control type="progress" id="23">
				<posx>106</posx>
				<posy>128</posy>
				<width>1068</width>
				<height>24</height>
				<texturebg>osd_progress_back_transparent.png</texturebg>
				<midtexture border="4">osd_progress_mid_normal.png</midtexture>
				<info>Player.Progress</info>
			</control>

			<!-- Progress Cache -->
			<control type="progress" id="24">
				<posx>100</posx>
				<posy>130</posy>
				<width>1080</width>
				<height>19</height>
				<texturebg>osd_progress_back_transparent.png</texturebg>
				<righttexture>osd_progress_line.png</righttexture>
				<info>Player.ProgressCache</info>
				<colordiffuse>mainblue</colordiffuse>
			</control>

			<!-- Progress Time -->
			<control type="label">
				<posx>90</posx>
				<posy>128</posy>
				<width>300</width>
				<height>20</height>
				<align>right</align> 
				<aligny>center</aligny>
				<font>Font_OSD_Time</font>
				<textcolor>mainblue</textcolor>
				<label>$INFO[VideoPlayer.Time(h:mm:ss)]</label>
				<animation effect="slide" start="0,0" end="-10,0" time="0" tween="cubic" easing="inout" condition="VideoPlayer.Content(episodes) + ![IsEmpty(Control.GetLabel(7657)) + IsEmpty(Control.GetLabel(7658))]">Conditional</animation>
			</control>
			<control type="label">
				<posx>1190</posx>
				<posy>128</posy>
				<width>300</width>
				<height>20</height>
				<aligny>center</aligny>
				<font>Font_OSD_Time</font>
				<textcolor>ffffffff</textcolor>
				<label>$INFO[VideoPlayer.Duration(h:mm:ss)]</label>
				<animation effect="slide" start="0,0" end="-304,0" time="0" tween="cubic" easing="inout" condition="VideoPlayer.Content(episodes) + ![IsEmpty(Control.GetLabel(7657)) + IsEmpty(Control.GetLabel(7658))]">Conditional</animation>
			</control>
		</control>

		<!-- Seekbar Cover/Thumb -->
		<control type="group">
			<posy>560</posy>
			<include>Animation_CommonFade_600</include>
			<visible>!Skin.HasSetting(OSD_Cover) + ![ Skin.String(osd_mode,Off) | Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231]) ]</visible>
                        <animation effect="slide" start="0,0" end="0,-90" time="800" tween="back" easing="inout" condition="Skin.String(osd_mode,Bottom)">Conditional</animation>
                        <animation effect="slide" start="0,0" end="0,20" time="800" tween="back" easing="inout" condition="Skin.String(osd_mode,Top)">Conditional</animation>

			<!-- Episodes -->
			<control type="group">
				<visible>VideoPlayer.Content(episodes)</visible>
				<!-- Preview Image -->
				<control type="image"> 
					<posx>960</posx>     
					<posy>-220</posy>    
					<width>300</width>
					<height>340</height>
					<aspectratio align="right" aligny="bottom">keep</aspectratio>
					<texture>$INFO[VideoPlayer.Cover]</texture>
					<bordertexture border="5">confluence/button-nofocus.png</bordertexture>
					<fadetime>400</fadetime>
					<bordersize>5</bordersize>
					<visible>!Skin.HasSetting(OSD_Clearart)</visible>
				</control>
				<!-- Clearart (PLEX) -->
				<control type="image"> 
					<posx>960</posx>     
					<posy>-25</posy>    
					<width>300</width>
					<height>120</height>
					<aspectratio aligny="bottom">scale</aspectratio>
					<texture fallback="backgrounds/fallback.png">tvlogos/$INFO[Videoplayer.TvShowTitle].png</texture>
					<fadetime>400</fadetime>
					<visible>!Skin.HasSetting(XBMC) + Skin.HasSetting(OSD_Clearart)</visible>
				</control>
				<!-- Clearart (XBMC) -->
				<control type="image"> 
					<posx>960</posx>     
					<posy>-60</posy>    
					<width>300</width>
					<height>170</height>
					<aspectratio aligny="bottom">scale</aspectratio>
					<texture fallback="backgrounds/fallback.png">$INFO[Player.Art(tvshow.clearart)]</texture>
					<fadetime>400</fadetime>
					<visible>Skin.HasSetting(XBMC) + Skin.HasSetting(OSD_Clearart) + !isEmpty(Player.Art(tvshow.clearart))</visible>
				</control>
			</control>

			<control type="image"> 
				<posx>960</posx>     
				<posy>-186</posy>    
				<width>300</width>
				<height>300</height>
				<aspectratio align="right" aligny="bottom">keep</aspectratio>
				<texture>$INFO[VideoPlayer.Cover]</texture>
				<bordertexture border="5">confluence/button-nofocus.png</bordertexture>
				<fadetime>400</fadetime>
				<bordersize>5</bordersize>
				<visible>VideoPlayer.Content(movies)</visible>
			</control>
			<control type="image"> 
				<posx>960</posx>     
				<posy>-220</posy>    
				<width>300</width>
				<height>340</height>
				<aspectratio align="right" aligny="bottom">keep</aspectratio>
				<texture>$INFO[VideoPlayer.Cover]</texture>
				<bordertexture border="5">confluence/button-nofocus.png</bordertexture>
				<fadetime>400</fadetime>
				<bordersize>5</bordersize>
				<visible>![ VideoPlayer.Content(movies) | VideoPlayer.Content(episodes) ]</visible>
			</control>


		</control>

		<!-- Seekbar Infos -->
		<control type="group">
			<include>Animation_CommonFade_600</include>
			<posx>20</posx>
			<posy>595</posy>
			<visible>![ Skin.String(osd_mode,Off) | Skin.String(osd_mode,Minimal) | Skin.String(osd_mode,Top) | Skin.String(osd_mode,Bottom) | Skin.String(osd_mode,Auto) + !SubString(VideoPlayer.SubtitleStream,$LOCALIZE[231]) ]</visible>

			<!-- TV  -->
			<control type="label">
				<posx>0</posx>
				<posy>-5</posy>
				<width>940</width> 
				<height>34</height>
				<label>$INFO[VideoPlayer.TVShowTitle]</label>
				<aligny>center</aligny>
				<font>Font_OSD_Header</font>
				<textcolor>mainblue</textcolor>
				<visible>VideoPlayer.Content(episodes)</visible>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>34</posy>
				<width>940</width> 
				<height>24</height> 
				<label>$LOCALIZE[20373] $INFO[VideoPlayer.season][COLOR=mainblue] / [/COLOR]$LOCALIZE[20359] $INFO[VideoPlayer.episode]</label>
				<aligny>center</aligny> 
				<font>Font_OSD_Sub</font>
				<textcolor>ffffffff</textcolor>
				<visible>VideoPlayer.Content(episodes)</visible>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>55</posy>
				<width>940</width>
				<height>24</height>
				<label>$INFO[VideoPlayer.Title]</label>
				<textcolor>ffffffff</textcolor>
				<font>Font_OSD_Sub</font>
				<align>center</align>
				<visible>VideoPlayer.Content(episodes)</visible>
			</control>

			<!-- Movies -->
			<control type="label">
				<posx>0</posx>
				<posy>-5</posy>
				<width>940</width> 
				<height>34</height> 
				<label>$INFO[VideoPlayer.Title]</label>
				<align>left</align> 
				<aligny>center</aligny> 
				<font>Font_OSD_Header</font>
				<textcolor>mainblue</textcolor>
				<visible>VideoPlayer.Content(movies)</visible>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>34</posy>
				<width>940</width> 
				<height>24</height> 
				<label>$INFO[VideoPlayer.Year]</label>
				<align>left</align> 
				<aligny>center</aligny> 
				<font>Font_OSD_Sub</font>
				<textcolor>ffffffff</textcolor>
				<visible>VideoPlayer.Content(movies)</visible>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>55</posy>
				<width>940</width>
				<height>24</height>
				<label>$LOCALIZE[21396]: $INFO[Player.Chapter] / $INFO[Player.ChapterCount]</label><!-- Chapter -->
				<textcolor>ffffffff</textcolor>
				<font>Font_OSD_Sub</font>
				<align>center</align>
				<visible>Player.ChapterCount + VideoPlayer.Content(movies)</visible>
			</control>

			<!-- Other -->
			<control type="label">
				<posx>0</posx>
				<posy>-5</posy>
				<width>940</width> 
				<height>34</height> 
				<label>$INFO[VideoPlayer.Title]</label>
				<align>left</align> 
				<aligny>center</aligny> 
				<font>Font_OSD_Header</font>
				<textcolor>mainblue</textcolor>
				<visible>![ VideoPlayer.Content(movies) | VideoPlayer.Content(episodes) ]</visible>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>55</posy>
				<width>940</width>
				<height>24</height>
				<label>$LOCALIZE[21396]: $INFO[Player.Chapter] / $INFO[Player.ChapterCount]</label><!-- Chapter -->
				<textcolor>ffffffff</textcolor>
				<font>Font_OSD_Sub</font>
				<align>center</align>
				<visible>Player.ChapterCount + ![ VideoPlayer.Content(movies) | VideoPlayer.Content(episodes) ]</visible>
			</control>
		</control>
	</control>
</include>

<!-- VIDEO OSD LABELS -->
<include name="OSD_Labels">
	<control type="group">
		<posy>200</posy>
		<visible>![Skin.HasSetting(OSD_Label) | Window.IsActive(OSDVideoSettings) | Window.IsActive(OSDAudioSettings) | Window.IsActive(125) | Window.IsActive(10205) | Window.IsActive(10204) | Window.IsActive(1120)]</visible>
		<include>Animation_CommonFade_600</include>

		<control type="group">

			<!-- PAUSED -->
			<control type="label" id="0">
				<posx>300</posx>
				<width>680</width>
				<align>center</align>
				<label>112</label> <!-- Paused -->
				<font>Font_Bartowski_OSD</font>
				<textcolor>FFCCCCCC</textcolor>
				<visible>player.paused + !player.caching</visible>
				<include>Animation_CommonFade_600</include>
			</control>
			<control type="label" id="0">
				<posx>300</posx>
				<width>680</width>
				<align>center</align>
				<label>112</label> <!-- Paused -->
				<font>Font_Bartowski_OSD</font>
				<textcolor>mainblue</textcolor>
				<visible>!Skin.HasSetting(osd_flash) + player.paused + !player.caching</visible>
				<include>Animation_CommonFade_600</include>
				<include>Animation_OSD</include>
			</control>
		</control>

		<!-- Remaining/Finish Time -->
		<control type="group">
			<visible>Skin.HasSetting(OSD_Big_Time)</visible>

			<!-- Remaining Time -->
			<control type="group">
				<posy>30</posy>
				<posx>20</posx>

				<control type="label" id="0">
					<width>280</width>
					<align>left</align>
					<label>$INFO[Player.TimeRemaining(hh:mm)]</label><!-- Remaining Time -->
					<font>Font_Bartowski_OSD_Small</font>
					<textcolor>FFCCCCCC</textcolor>
					<visible>player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
				</control>
				<control type="label" id="0">
					<width>280</width>
					<align>left</align>
					<label>$INFO[Player.TimeRemaining(hh:mm)]</label><!-- Remaining Time -->
					<font>Font_Bartowski_OSD_Small</font>
					<textcolor>mainblue</textcolor>
					<visible>!Skin.HasSetting(osd_flash) + player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
					<include>Animation_OSD</include>
				</control>

				<!-- Labels -->
				<control type="label" id="0">
					<posy>90</posy>
					<width>280</width>
					<align>left</align>
					<label>31086</label><!-- Remaining Time -->
					<font>Font_Time_Global</font>
					<textcolor>FFCCCCCC</textcolor>
					<visible>player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
				</control>
				<control type="label" id="0">
					<posy>90</posy>
					<width>280</width>
					<align>left</align>
					<label>31086</label><!-- Remaining Time -->
					<font>Font_Time_Global</font>
					<textcolor>mainblue</textcolor>
					<visible>!Skin.HasSetting(osd_flash) + player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
					<include>Animation_OSD</include>
				</control>
			</control>

			<!-- Finish Time -->
			<control type="group">
				<posx>1260</posx>
				<posy>30</posy>
				<control type="label" id="0">
					<width>280</width>
					<align>right</align>
					<label>$INFO[Player.FinishTime(hh:mm)]</label><!-- Finish Time -->
					<font>Font_Bartowski_OSD_Small</font>
					<textcolor>FFCCCCCC</textcolor>
					<visible>player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
				</control>
				<control type="label" id="0">
					<width>280</width>
					<align>right</align>
					<label>$INFO[Player.FinishTime(hh:mm)]</label><!-- Finish Time -->
					<font>Font_Bartowski_OSD_Small</font>
					<textcolor>mainblue</textcolor>
					<visible>!Skin.HasSetting(osd_flash) + player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
					<include>Animation_OSD</include>
				</control>

				<!-- Labels -->
				<control type="label" id="0">
					<posy>90</posy>
					<width>280</width>
					<align>right</align>
					<label>31087</label><!-- Finish Time -->
					<font>Font_Time_Global</font>
					<textcolor>FFCCCCCC</textcolor>
					<visible>player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
				</control>
				<control type="label" id="0">
					<posy>90</posy>
					<width>280</width>
					<align>right</align>
					<label>31087</label><!-- Finish Time -->
					<font>Font_Time_Global</font>
					<textcolor>mainblue</textcolor>
					<visible>!Skin.HasSetting(osd_flash) + player.paused + !player.caching</visible>
					<include>Animation_CommonFade_600</include>
					<include>Animation_OSD</include>
				</control>
			</control>
		</control>

		<!-- Current Time -->
		<control type="group">
			<posx>0</posx>
			<posy>180</posy>
			<visible>Skin.HasSetting(OSD_Big_Time_Current)</visible>
			<control type="label" id="0">
				<width>1280</width>
				<align>center</align>
				<label>$INFO[System.Time(hh:mm xx)]</label><!-- Current Time -->
				<font>Font_Bartowski_OSD_Small</font>
				<textcolor>FFCCCCCC</textcolor>
				<visible>player.paused + !player.caching</visible>
				<include>Animation_CommonFade_600</include>
			</control>
			<control type="label" id="0">
				<width>1280</width>
				<align>center</align>
				<label>$INFO[System.Time(hh:mm xx)]</label><!-- Current Time -->
